<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %><br>
  <%= f.label :artist_name %>
  <!-- <%= f.text_field :artist_name %><br>
  <%= f.hidden_field :artist_id %><br> -->
  <!-- has a selector for artist -->
  <%= f.label "Select Existing Artist" %><br>
  <%= artist_select(@song, @song.artist) %>
  <br>
  <br>
  <%= f.submit %>
<% end %>
